{
  "name": "react-new",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.3",
    "bootstrap": "^4.5.2",
    "electron-is-dev": "^1.2.0",
    "extract-zip": "^2.0.1",
    "n-readlines": "^1.0.0",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "sqlite3": "^5.0.0"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test --coverage",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder build -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "concurrently": "^5.3.0",
    "electron": "^10.1.0",
    "electron-builder": "^22.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint-config-google": "^0.14.0",
    "fake-indexeddb": "^3.1.2",
    "nock": "^13.0.4",
    "react-test-renderer": "^16.13.1",
    "tmp": "^0.2.1",
    "wait-on": "^5.2.0"
  },
  "author": {
    "name": "Ossian Gustafsson",
    "email": "ogustafsson1@gmail.com"
  },
  "build": {
    "appId": "com.addonclient.client",
    "productName": "Add0nClient",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/{db,services}/*.js",
      "src/react/*.js",
      "src/react/components/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
